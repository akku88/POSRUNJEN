		{
			"name": "1. create user using curl from swaggerdoc",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"var randmNum = Math.floor(Math.random()*100+1);",
							"var randmStr =  \"susan\" + Math.random().toString(36).substring();",
							"",
							"pm.environment.set(\"id\",randmNum);",
							"pm.collectionVariables.set(\"username\",randmStr);",
							"pm.collectionVariables.set(\"firstName\", \"Susanna\");",
							"pm.collectionVariables.set(\"lastName\", \"Thomas\");",
							"pm.collectionVariables.set(\"email\", randmStr + \"@gmail.com\");",
							"pm.collectionVariables.set(\"password\", randmStr);",
							"pm.collectionVariables.set(\"phone\", Math.random()*1000000000);",
							"pm.collectionVariables.set(\"status\", 0);",
							"",
							"console.log(randmNum);",
							"console.log(randmStr);",
							"",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Check status is 200\", function(){",
							"",
							"pm.response.to.have.status(200);",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "accept",
						"value": "application/xml"
					},
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"id\": \"{{id}}\",\n  \"username\": \"{{username}}\",\n  \"firstName\": \"{{firstName}}\",\n  \"lastName\": \"{{lastName}}\",\n  \"email\": \"{{email}}\",\n  \"password\": \"{{password}}\",\n  \"phone\": \"{{phone}}\",\n  \"userStatus\": \"{{status}}\"\n}",
					"options": {
